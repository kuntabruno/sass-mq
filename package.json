{
  "name": "@mcaskill/sass-mq",
  "description": "mq()+ is a Sass library that helps manipulating media queries in an elegant way.",
  "version": "5.0.1",
  "keywords": [
    "responsive",
    "sass",
    "rwd",
    "mediaquery",
    "query",
    "queries",
    "media",
    "eyeglass-module"
  ],
  "ignore": [
    "test",
    "sassdoc",
    "**/.*",
    "sache.json",
    "bower.json",
    "sassdoc.sh",
    "scripts"
  ],
  "files": [
    "_mq.scss",
    "LICENSE.md",
    "README.md",
    "sass/index.scss"
  ],
  "main": "_mq.scss",
  "scripts": {
    "test": "./scripts/test.sh",
    "sassdoc": "./scripts/sassdoc.sh"
  },
  "contributors": [
    {
      "name": "Chauncey McAskill",
      "email": "chauncey@mcaskill.ca",
      "web": "https://mcaskill.ca"
    },
    {
      "name": "Guardian Media Group and Contributors",
      "web": "https://github.com/sass-mq/sass-mq"
    }
  ],
  "bugs": {
    "url": "https://github.com/mcaskill/sass-mq/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcaskill/sass-mq.git"
  },
  "license": "MIT",
  "homepage": "https://mcaskill.github.io/sass-mq/",
  "eyeglass": {
    "needs": ">= 0.8.2",
    "sassDir": "./_mq.scss",
    "exports": false
  },
  "devDependencies": {
    "eyeglass": "^2.4.1",
    "node-sass": "^4.12.0",
    "sassdoc": "^2.7.0"
  }
}
